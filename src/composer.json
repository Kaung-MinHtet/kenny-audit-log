{
    "name": "kenny/audit-log",
    "description": "A simple audit logging package for Laravel models.",
    "type": "laravel-package",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kenny\\AuditLog\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kenny\\AuditLog\\Providers\\AuditLogServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
